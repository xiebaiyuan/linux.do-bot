# 复制此文件为 .env 并填写你的实际配置

# 登录凭据
LINUXDO_USERNAME=your_username
LINUXDO_PASSWORD=your_password

# 行为概率设置 (0-1之间的小数)
LIKE_PROBABILITY=0.02
REPLY_PROBABILITY=0.00
COLLECT_PROBABILITY=0.02

# 其他设置
MAX_TOPICS=10

# 网站URL (通常保持默认即可)
HOME_URL=
CONNECT_URL=

# WxPusher推送设置 (可选)
USE_WXPUSHER=false
APP_TOKEN=
TOPIC_ID=

# 容器设置
IMMEDIATE_RUN=false
